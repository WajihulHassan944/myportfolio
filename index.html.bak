<script>

var userName = document.getElementById("namess");
var userEmail = document.getElementById("emailss");
var userMessage = document.getElementById("messageBody");
var userSubject = document.getElementById("subjectss");
var orderFormSubmit = document.getElementById("newOrderForm");
var loadingAnimation = document.getElementById("loadingAnimation");
const body = document.querySelector("body");

orderFormSubmit.addEventListener("submit", function(event) {
  event.preventDefault(); 


  loadingAnimation.style.display = "block";

  var formData = {
    name: userName.value,
    email: userEmail.value,
    message: userMessage.value,
    subject: userSubject.value,
  };

  fetch("https://mental-health-backend-one.vercel.app/send-data-myportfolio", {
    method: "POST",
    headers: {
      "Content-Type": "application/json"
    },
    body: JSON.stringify(formData)
  })
  .then(function(response) {
    if (response.ok) {
      alert("Message submitted successfully.");
      loadingAnimation.style.display = "none";
      location.reload();
    } else {
      console.log("Error submitting message.");
      loadingAnimation.style.display = "none";
    }
  })
  .catch(function(error) {
    console.log("Error submitting message.");
    loadingAnimation.style.display = "none";
    console.error(error);
  });
});

</script>











